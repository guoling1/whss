<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jkm.hss.merchant.dao.RequestUrlParamDao">
    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO tb_request_url_param (request_url) values (#{requestUrl});
    </insert>
    <select id="getRequestUrlById" resultType="java.lang.String">
        SELECT request_url as requestUrl
        FROM tb_request_url_param
        WHERE id=#{id};
    </select>
</mapper>