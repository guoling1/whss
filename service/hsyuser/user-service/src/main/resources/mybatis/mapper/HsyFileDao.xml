<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jkm.hsy.user.dao.HsyFileDao">
    <insert id="insert" useGeneratedKeys="true" keyProperty="uuid" parameterType="com.jkm.hsy.user.entity.AppCmFile">
        insert into app_cm_file(UUID,FILENAME,FILETYPE,PATH,CREATETIME,UPDATETIME) values(#{uuid},#{fileName},#{fileType},#{path},#{createTime},#{updateTime})
    </insert>
</mapper>